					<key>COMMAND_STRING</key>
					<string># Script by Sengoku contact:support@donxj.com

# H264压制 自定义 v1.0
# pkg VideoService
# -----------------------------
version="H.264压制 自定义参数 PKG v1.0"
# ffmpeg 地址
ffmpeg=/Applications/sengoku.app/ffmpeg

if  ( [ -f $ffmpeg ] ) ; then
    # 设置CRF参数
    CRF="$(osascript -e 'display dialog "输入视频质量参数 CRF\n\n 👉 CRF 可选范围:0-51(0代表无损,51最差质量,默认值 28)\n 👉 主观上合理的范围是:17-28" default answer "28" with title "'"$version"'"' -e 'text returned of result' 2&gt;/dev/null)"
    # 若取消则退出，若留空则使用默认值
    if [ $? -ne 0 ]; then
        osascript -e 'display notification "已取消输入，若想转换 MP4 请重新运行。" with title "用户取消🙁" sound name "Basso"'
        exit 0
    elif [ -z "$CRF" ]; then
        CRF="28"
    fi
    # 设置视频尺寸
    SIZE="$(osascript -e 'display dialog "输入视频尺寸:\n\n 👉 输入高度,宽度自适应,默认值 720\n 👉 iw-原始宽度,ih-原始高度" default answer "720" with title "'"$version"'"' -e 'text returned of result' 2&gt;/dev/null)"
    # 若取消则退出，若留空则使用默认值
    if [ $? -ne 0 ]; then
        osascript -e 'display notification "已取消输入，若想转换 MP4 请重新运行。" with title "用户取消🙁" sound name "Basso"'
        exit 0
    elif [ -z "$SIZE" ]; then
        SIZE="720"
    fi
    # 循环获取文件
    for f in "$@"
    do
      # 获取所在文件夹
      dir_path="$(dirname "$f")"
      # 获取文件名
      file_name="$(basename "$f")"
      # 输出路径
      output_name="${f%.*}_x264_CRF"$CRF"_"$SIZE"p.mp4"
      # 开始压制
        "$ffmpeg" -i "$f" -y -c:v libx264 -preset 8 -x264opts crf=$CRF:keyint=infinite:min-keyint=1:scenecut=60:partitions=all:direct=auto:me=umh:merange=32:subme=10:trellis=2:rc-lookahead=60:ref=6:bframes=6:b-adapt=2:deblock=1,1:qcomp=0.5:psy-rd=0.3,0:aq-mode=2:aq-strength=0.8 -r 25 -vf "scale='-2':'min($SIZE,ih)':flags=lanczos" -pix_fmt yuv420p -c:a aac -b:a 128k -ar 44100 -y "$output_name"
    done

    # 放个通知告诉你压完了
    if ( [ -f "$output_name" ]) ; then
        osascript -e 'display notification "CRF质量=23.5;FPS帧速率=25;尺寸=原始尺寸" with title "转换完成🎉" subtitle "FFmpeg converter" sound name "Glass"'
    else
        osascript -e 'display notification "⚠️视频压制失败，请联系 support@donxj.com" with title "视频压制失败‼️" subtitle "FFmpeg converter" sound name "Basso"'
    fi
else
# 找不到 ffmpeg 后提示
  afplay "/System/Library/Sounds/Basso.aiff"
	osascript -e 'display dialog "未找到 FFMPEG 主程序，请联系 support@donxj.com" buttons {"OK"} default button 1 with icon caution with title "⚠️出错了！"'
fi</string>
