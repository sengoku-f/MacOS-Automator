					<string># 视频转换为 GIF pkg v1.0
# pkg VideoService
# 更新修复竖版视频高度
# 更新为 ffmpeg
#-----------------------------------------------
# ffmpeg 地址
ffmpeg=/Applications/sengoku.app/ffmpeg
# gifski=/Applications/sengoku.app/gifski
gifsicle=/Applications/sengoku.app/gifsicle

if  ( [ -f $ffmpeg -a -f $gifsicle ] ) ; then
    # 循环获取文件
    for f in "$@"
    do
      dir="$(dirname "$f")"
      name="$(basename "$f")"
      # 输出路径
      output_name="${name%.*}.gif"

      cd "$dir"

      mkdir .temp

      "$ffmpeg" -i "$f" -vf "fps=25,scale=iw*min(1\,min(480/iw\,480/ih)):-2:flags=lanczos,split[s0][s1];[s0]palettegen[p];[s1][p]paletteuse" -loop 0 -y ".temp.gif"

      # "$gifski" --quality 480 --width 480 --height 480 --fast -o .temp.gif .temp/*.png

      "$gifsicle" -O3 --lossy=80 --colors 256 .temp.gif &gt; "$output_name"
      
      # 清理
      rm -rf .temp
      rm -rf .temp.gif

    done

    # 放个通知告诉你压完了
    if ( [ -f "$output_name" ]) ; then
        osascript -e 'display notification "FFmpeg converter" with title "转换完成🎉" sound name "Glass"'
    else
        osascript -e 'display notification "⚠️转换失败，请联系 support@donxj.com" with title "GIF转换失败‼️" sound name "Basso"'
    fi
else
# 找不到 ffmpeg 后提示
  afplay "/System/Library/Sounds/Basso.aiff"
	osascript -e 'display dialog "未找到 FFMPEG GIFSICLE 主程序，请联系 support@donxj.com" buttons {"OK"} default button 1 with icon caution with title "⚠️出错了！"'
fi</string>
