					<string>PATH=$PATH:/usr/local/bin/:/opt/homebrew/bin
for f in "$@"
do
	dir="$(dirname "$f")"
	name="$(basename "$f")"

	cd "$f"
	#echo "$f"

	# 处理含有空格的文件名

	#find "." -depth -name "* *" -execdir rename 's/ //g' "{}" \;

	# 创建文件夹

	mkdir "@1x" "@2x" "@3x"
	
	# 执行分类xargs多线程
	
	find "." -type f -name "*@2x*" -maxdepth 1 -mindepth 1 -print0 | xargs -0 -P 8 -J {} mv -f "{}" "./@2x"
	find "." -type f -name "*@3x*" -maxdepth 1 -mindepth 1 -print0 | xargs -0 -P 8 -J {} mv -f "{}" "./@3x"
	find "." -type f -name "*.png" -maxdepth 1 -mindepth 1 -print0 | xargs -0 -P 8 -J {} mv -f "{}" "./@1x" &amp;&amp; find "." -type f -name "*.jpg" -maxdepth 1 -mindepth 1 -print0 | xargs -0 -P 8 -J {} mv -f "{}" "./@1x"
    
	# 删除空文件夹
    find "." -depth -type d -empty -exec rmdir {} \;
done

# 播放提示音

#afplay "/System/Library/Sounds/Glass.aiff"

# 发送通知并播放提示音

osascript -e 'display notification "切图分类 by Sengoku v1.0" with title "所有文件已分类🎉" sound name "Glass"'
