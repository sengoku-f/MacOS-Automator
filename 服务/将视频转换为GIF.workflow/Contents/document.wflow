for f
do
	dir="$(dirname "$f")"
	name="$(basename "$f")"

	cd "$dir"

	mkdir .temp

	/usr/local/bin/ffmpeg -i "$f" -vf "scale=iw*min(1\,min(480/iw\,480/ih)):-1:flags=lanczos" ".temp/%08d.png"

	/usr/local/bin/convert +repage -fuzz 1.6% -delay 4 -loop 0 .temp/*.png -layers OptimizePlus -layers OptimizeTransparency .temp.gif

	/usr/local/bin/gifsicle -O3 --lossy=80 --colors 256 .temp.gif &gt; "${name%.*}.gif"
	
	# Ê∏ÖÁêÜ
	rm -rf .temp
	rm -rf .temp.gif

done

